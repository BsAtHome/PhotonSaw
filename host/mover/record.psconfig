<photonsaw-config>
  <movementConstraints>
    <axes>
      <movementconstraint axis="X">
        <acceleration>500.0</acceleration>
        <maxSpeed>1500.0</maxSpeed>
        <minSpeed>2.0</minSpeed>
        <maxJerk>2.0</maxJerk>
        <mmPerStep>0.0375</mmPerStep>
        <coilCurrent>350</coilCurrent>
        <microSteppingMode>3</microSteppingMode>
      </movementconstraint>
      <movementconstraint axis="Y">
        <acceleration>250.0</acceleration>
        <maxSpeed>600.0</maxSpeed>
        <minSpeed>1.0</minSpeed>
        <maxJerk>1.0</maxJerk>
        <mmPerStep>0.0375</mmPerStep>
        <coilCurrent>1850</coilCurrent>
        <microSteppingMode>3</microSteppingMode>
      </movementconstraint>
      <movementconstraint axis="Z">
        <acceleration>1.0</acceleration>
        <maxSpeed>4.0</maxSpeed>
        <minSpeed>1.0</minSpeed>
        <maxJerk>1.0</maxJerk>
        <mmPerStep>0.003125</mmPerStep>
        <coilCurrent>350</coilCurrent>
        <microSteppingMode>1</microSteppingMode>
      </movementconstraint>
      <movementconstraint axis="A">
        <acceleration>5000.0</acceleration>
        <maxSpeed>10.0</maxSpeed>
        <minSpeed>1.0</minSpeed>
        <maxJerk>50.0</maxJerk>
        <mmPerStep>0.0375</mmPerStep>
        <coilCurrent>350</coilCurrent>
        <microSteppingMode>3</microSteppingMode>
      </movementconstraint>
    </axes>
    <junctionDeviation>0.1</junctionDeviation>
    <rapidMoveSpeed>5000.0</rapidMoveSpeed>
    <tickHZ>50000</tickHZ>
    <shortestMove>0.1</shortestMove>
  </movementConstraints>
  <jettyConfig>
    <port>8080</port>
  </jettyConfig>
  <hostConfig>
    <serialPort>/dev/ttyACM0</serialPort>
    <recordDir>simulation</recordDir>
    <simulating>false</simulating>
    <recording>true</recording>
    <jobsInMemory>10</jobsInMemory>
  </hostConfig>
</photonsaw-config>