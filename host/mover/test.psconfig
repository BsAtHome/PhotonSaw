    <junctionDeviation>0.1</junctionDeviation>
    <rapidMoveSpeed>5000.0</rapidMoveSpeed>
    <tickHZ>50000</tickHZ>
    <shortestMove>0.1</shortestMove>
  </movementConstraints>
  <jettyConfig>
    <port>8080</port>
  </jettyConfig>
  <hostConfig>
    <serialPort>/dev/ttyACM0</serialPort>
    <simulating>false</simulating>
    <recording>false</recording>
    <jobsInMemory>0</jobsInMemory>
  </hostConfig>
</photonsaw-config>